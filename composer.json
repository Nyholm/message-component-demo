{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1.3",
        "ext-iconv": "*",
        "doctrine/doctrine-bundle": "^1.6.10",
        "doctrine/doctrine-migrations-bundle": "^1.3",
        "doctrine/orm": "^2.5.11",
        "enqueue/amqp-ext": "^0.8.11",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/console": "^4.0",
        "symfony/debug-pack": "^1.0",

        "symfony/framework-bundle": "^4.1@dev",
        "symfony/messenger": "^4.1@dev",
        "symfony/monolog-bundle": "^3.1",
        "symfony/serializer-pack": "dev-master",
        "symfony/web-server-bundle": "^4.0",
        "symfony/yaml": "^4.0",
        "symfony/symfony": "dev-master"
    },
    "require-dev": {
        "symfony/dotenv": "^4.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "extra": {
        "symfony": {
            "id": "01C38Z7F737WGJJFV4MR6GK2SF",
            "allow-contrib": "true"
        }
    }
}
